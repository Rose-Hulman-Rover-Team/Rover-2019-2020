<?xml version='1.0'?>
<robot name="dd_robot">

  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <box size="2.0 0.8 0.25"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0.5 1 1"/>
      </material>
    </visual>
  </link>

  <!-- Right Wheel1 -->
  <link name="right_wheel1">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.1" radius="0.2" />
      </geometry>
      <matireal name="black">
        <color rgba="0.05 0.05 0.05 1"/>
      </material>
    </visual>
  </link>
  <joint name="joint_right_wheel1" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel1"/>
    <origin xyz="0.8 -0.45 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" />
  </joint>

  <!-- Left Wheel1 -->
  <link name="left_wheel1">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="joint_left_wheel1" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel1"/>
    <origin xyz="0.8 0.45 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" />
  </joint>

    <!-- Right Wheel2 -->
  <link name="right_wheel2">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
