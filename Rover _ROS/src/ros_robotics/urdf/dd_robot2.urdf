          <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="joint_right_wheel2" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel2"/>
    <origin xyz="0 -0.45 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" />
  </joint>

  <!-- Left Wheel2 -->
  <link name="left_wheel2">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="joint_left_wheel2" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel2"/>
    <origin xyz="0 0.45 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" />
  </joint>

    <!-- Right Wheel3 -->
  <link name="right_wheel3">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="black"/>
